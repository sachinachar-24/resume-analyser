# Groq API key for AI explanations (optional - if missing, only vector scores will be shown)
GROQ_API_KEY=your_groq_api_key_here

# Minimum similarity score to trigger AI explanations (0.0-1.0)
EXPLANATION_THRESHOLD=0.6

# Maximum number of resumes/jobs to handle
MAX_SCALE=200

# Path to Qdrant local storage
QDRANT_PATH=./qdrant_storage
